{
    "name":"Canary",
    "version":"1.0",
    "manifest_version":2,
    
    "web_accessible_resources": [
        "data/*"
    ],

    
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js": ["resources/jquery.min.js", "build/content.js"]
        }
    ],
    
    "background": {
        "scripts":["resources/jquery.min.js", "background.js"]
    },


    "icons":{
        "128":"resources/Canary.png"
    }
   
}